{
  "name": "@quilted/aws",
  "type": "module",
  "version": "0.3.38",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/aws"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./build/typescript/index.d.ts",
      "quilt:source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./http-handlers": {
      "types": "./build/typescript/http-handlers.d.ts",
      "quilt:source": "./source/http-handlers.ts",
      "quilt:esnext": "./build/esnext/http-handlers.esnext",
      "import": "./build/esm/http-handlers.mjs",
      "require": "./build/cjs/http-handlers.cjs"
    },
    "./craft": {
      "types": "./build/typescript/craft.d.ts",
      "quilt:source": "./source/craft.ts",
      "quilt:esnext": "./build/esnext/craft.esnext",
      "import": "./build/esm/craft.mjs",
      "require": "./build/cjs/craft.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "http-handlers": [
        "./build/typescript/http-handlers.d.ts"
      ],
      "craft": [
        "./build/typescript/craft.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@types/aws-lambda": "^8.0.0",
    "common-tags": "^1.8.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "@quilted/quilt": "^0.5.106",
    "@quilted/craft": "^0.1.138"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "@quilted/craft": {
      "optional": true
    },
    "@quilted/quilt": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/project"
    ]
  }
}
