{
  "name": "@quilted/cloudflare",
  "type": "module",
  "version": "0.1.36",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/cloudflare"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "quilt:from-source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./http-handlers": {
      "quilt:from-source": "./source/http-handlers.tsx",
      "quilt:esnext": "./build/esnext/http-handlers.esnext",
      "import": "./build/esm/http-handlers.mjs",
      "require": "./build/cjs/http-handlers.cjs"
    },
    "./craft": {
      "quilt:from-source": "./source/craft.ts",
      "quilt:esnext": "./build/esnext/craft.esnext",
      "import": "./build/esm/craft.mjs",
      "require": "./build/cjs/craft.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "http-handlers": [
        "./build/typescript/http-handlers.d.ts"
      ],
      "craft": [
        "./build/typescript/craft.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@cloudflare/workers-types": "^3.7.1",
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "common-tags": "^1.8.0",
    "mime": "^2.5.0",
    "miniflare": "^2.4.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "@quilted/craft": "^0.1.121",
    "@quilted/quilt": "^0.5.102"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/mime": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "@quilted/craft": {
      "optional": true
    },
    "@quilted/quilt": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/node",
      "@quilted/eslint-config/package"
    ]
  }
}
