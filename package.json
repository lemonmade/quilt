{
  "name": "quilt",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/lemonmade/quilt.git",
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "postinstall": "lerna bootstrap",
    "test": "sk test",
    "lint": "sk lint",
    "type-check": "sk type-check --no-cache",
    "build": "sk build",
    "prerelease": "yarn build",
    "release": "lerna publish && git push --tags"
  },
  "devDependencies": {
    "@sewing-kit/cli": "^0.0.13",
    "@sewing-kit/config": "^0.0.11",
    "@sewing-kit/eslint-plugin": "^0.0.3",
    "@sewing-kit/plugin-babel": "^0.0.14",
    "@sewing-kit/plugin-eslint": "^0.0.12",
    "@sewing-kit/plugin-javascript": "^0.0.15",
    "@sewing-kit/plugin-jest": "^0.0.15",
    "@sewing-kit/plugin-json": "^0.0.15",
    "@sewing-kit/plugin-package-base": "^0.0.15",
    "@sewing-kit/plugin-package-commonjs": "^0.0.15",
    "@sewing-kit/plugin-package-esmodules": "^0.0.15",
    "@sewing-kit/plugin-package-esnext": "^0.0.15",
    "@sewing-kit/plugin-package-typescript": "^0.0.16",
    "@sewing-kit/plugin-react": "^0.0.10",
    "@sewing-kit/plugin-typescript": "^0.0.15",
    "lerna": "^3.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
