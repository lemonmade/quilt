{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": ["middlewares", "pnpm", "Vite"],
  "cSpell.ignoreWords": [
    "Changesets",
    "browserslist",
    "preloadable",
    "unmount"
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/build": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
