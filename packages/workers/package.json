{
  "name": "@quilted/workers",
  "type": "module",
  "version": "0.2.13",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/workers"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "quilt:from-source": "./src/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./worker": {
      "quilt:from-source": "./src/worker.ts",
      "quilt:esnext": "./build/esnext/worker.esnext",
      "import": "./build/esm/worker.mjs",
      "require": "./build/cjs/worker.cjs"
    },
    "./babel": {
      "import": "./build/esm/babel.mjs",
      "require": "./build/cjs/babel.cjs"
    },
    "./rollup": {
      "quilt:from-source": "./src/rollup-parts.ts",
      "quilt:esnext": "./build/esnext/rollup-parts.esnext",
      "import": "./build/esm/rollup-parts.mjs",
      "require": "./build/cjs/rollup-parts.cjs"
    },
    "./sewing-kit": {
      "quilt:from-source": "./src/sewing-kit.ts",
      "quilt:esnext": "./build/esnext/sewing-kit.esnext",
      "import": "./build/esm/sewing-kit.mjs",
      "require": "./build/cjs/sewing-kit.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "worker": [
        "./build/typescript/worker.d.ts"
      ],
      "babel": [
        "./build/typescript/babel.d.ts"
      ],
      "sewing-kit": [
        "./build/typescript/sewing-kit.d.ts"
      ],
      "rollup": [
        "./build/typescript/rollup-parts.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@remote-ui/rpc": "^1.2.0",
    "common-tags": "^1.8.0"
  },
  "devDependencies": {
    "@quilted/sewing-kit": "^0.2.0",
    "@quilted/sewing-kit-babel": "^0.1.0",
    "@quilted/sewing-kit-rollup": "^0.1.0",
    "@quilted/sewing-kit-vite": "^0.1.0"
  },
  "peerDependencies": {
    "@babel/core": ">=7.0.0 <8.0.0",
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "@quilted/sewing-kit": "^0.2.14",
    "@quilted/sewing-kit-babel": "^0.1.8",
    "@quilted/sewing-kit-rollup": "^0.1.8",
    "@quilted/sewing-kit-vite": "^0.1.3",
    "rollup": ">=2.50.3 <3.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "@babel/runtime": {
      "optional": true
    },
    "@quilted/sewing-kit": {
      "optional": true
    },
    "@quilted/sewing-kit-babel": {
      "optional": true
    },
    "@quilted/sewing-kit-rollup": {
      "optional": true
    },
    "@quilted/sewing-kit-vite": {
      "optional": true
    },
    "rollup": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/package"
    ]
  }
}
