{
  "name": "@quilted/workers",
  "type": "module",
  "version": "0.2.32",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/workers"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./build/typescript/index.d.ts",
      "quilt:source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./babel": {
      "types": "./build/typescript/babel.d.ts",
      "import": "./build/esm/babel.mjs",
      "require": "./build/cjs/babel.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "babel": [
        "./build/typescript/babel.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@quilted/threads": "^0.1.0",
    "common-tags": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": ">=7.0.0 <8.0.0",
    "@types/babel__core": ">=7.0.0 <8.0.0",
    "@types/babel__template": ">=7.0.0 <8.0.0",
    "@types/babel__traverse": ">=7.0.0 <8.0.0",
    "@types/common-tags": "^1.8.0"
  },
  "peerDependencies": {
    "@babel/core": ">=7.0.0 <8.0.0",
    "@babel/runtime": ">=7.0.0 <8.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "@babel/runtime": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/project"
    ]
  }
}
