{
  "name": "@quilted/aws",
  "type": "module",
  "version": "0.2.38",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/aws"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "sewing-kit:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs"
    },
    "./http-handlers": {
      "sewing-kit:esnext": "./build/esnext/http-handlers.esnext",
      "import": "./build/esm/http-handlers.mjs"
    },
    "./sewing-kit": {
      "sewing-kit:esnext": "./build/esnext/sewing-kit.esnext",
      "import": "./build/esm/sewing-kit.mjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "http-handlers": [
        "./build/typescript/http-handlers.d.ts"
      ],
      "sewing-kit": [
        "./build/typescript/sewing-kit.d.ts"
      ],
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@quilted/http-handlers": "^0.1.11",
    "@types/aws-lambda": "^8.0.0",
    "common-tags": "^1.8.0",
    "cookie": "^0.4.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/cookie": "^0.4.0"
  },
  "peerDependencies": {
    "@quilted/craft": "^0.0.0",
    "@quilted/sewing-kit": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@quilted/craft": {
      "optional": true
    },
    "@quilted/sewing-kit": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "../eslint/node",
      "../eslint/package"
    ]
  }
}
