{
  "name": "@quilted/aws",
  "type": "module",
  "version": "0.3.27",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/aws"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "quilt:internal": "./src/index.ts",
      "sewing-kit:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./http-handlers": {
      "quilt:internal": "./src/http-handlers.ts",
      "sewing-kit:esnext": "./build/esnext/http-handlers.esnext",
      "import": "./build/esm/http-handlers.mjs",
      "require": "./build/cjs/http-handlers.cjs"
    },
    "./sewing-kit": {
      "quilt:internal": "./src/sewing-kit.ts",
      "sewing-kit:esnext": "./build/esnext/sewing-kit.esnext",
      "import": "./build/esm/sewing-kit.mjs",
      "require": "./build/cjs/sewing-kit.cjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "http-handlers": [
        "./build/typescript/http-handlers.d.ts"
      ],
      "sewing-kit": [
        "./build/typescript/sewing-kit.d.ts"
      ],
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@quilted/http-handlers": "^0.2.19",
    "@types/aws-lambda": "^8.0.0",
    "common-tags": "^1.8.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "@quilted/craft": "^0.1.9",
    "@quilted/sewing-kit": "^0.2.4"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "@quilted/craft": {
      "optional": true
    },
    "@quilted/sewing-kit": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/node",
      "@quilted/eslint-config/package"
    ]
  }
}
