{
  "name": "@quilted/http-handlers",
  "type": "module",
  "version": "0.2.40",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/http-handlers"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./build/typescript/index.d.ts",
      "quilt:from-source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./node": {
      "types": "./build/typescript/node/index.d.ts",
      "quilt:from-source": "./source/node/index.ts",
      "quilt:esnext": "./build/esnext/node/index.esnext",
      "import": "./build/esm/node/index.mjs",
      "require": "./build/cjs/node/index.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "node": [
        "./build/typescript/node/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@quilted/http": "^0.2.11",
    "@quilted/routing": "^0.2.7",
    "send": "^0.17.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/send": "^0.17.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/package"
    ]
  }
}
