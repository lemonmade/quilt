{
  "name": "@quilted/quilt",
  "type": "module",
  "version": "0.5.61",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/quilt"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "quilt:internal": "./src/index.ts",
      "sewing-kit:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./server": {
      "quilt:internal": "./src/server/index.ts",
      "sewing-kit:esnext": "./build/esnext/server/index.esnext",
      "import": "./build/esm/server/index.mjs",
      "require": "./build/cjs/server/index.cjs"
    },
    "./static": {
      "quilt:internal": "./src/static/index.tsx",
      "sewing-kit:esnext": "./build/esnext/static/index.esnext",
      "import": "./build/esm/static/index.mjs",
      "require": "./build/cjs/static/index.cjs"
    },
    "./html": {
      "quilt:internal": "./src/html.ts",
      "sewing-kit:esnext": "./build/esnext/html.esnext",
      "import": "./build/esm/html.mjs",
      "require": "./build/cjs/html.cjs"
    },
    "./email": {
      "quilt:internal": "./src/email.ts",
      "sewing-kit:esnext": "./build/esnext/email.esnext",
      "import": "./build/esm/email.mjs",
      "require": "./build/cjs/email.cjs"
    },
    "./global": {
      "quilt:internal": "./src/global.ts",
      "sewing-kit:esnext": "./build/esnext/global.esnext",
      "import": "./build/esm/global.mjs",
      "require": "./build/cjs/global.cjs"
    },
    "./http": {
      "quilt:internal": "./src/http.ts",
      "sewing-kit:esnext": "./build/esnext/http.esnext",
      "import": "./build/esm/http.mjs",
      "require": "./build/cjs/http.cjs"
    },
    "./testing": {
      "quilt:internal": "./src/testing.ts",
      "sewing-kit:esnext": "./build/esnext/testing.esnext",
      "import": "./build/esm/testing.mjs",
      "require": "./build/cjs/testing.cjs"
    },
    "./matchers": {
      "quilt:internal": "./src/matchers.ts",
      "sewing-kit:esnext": "./build/esnext/matchers.esnext",
      "import": "./build/esm/matchers.mjs",
      "require": "./build/cjs/matchers.cjs"
    },
    "./react/jsx-runtime": {
      "quilt:internal": "./src/react/jsx-runtime.ts",
      "sewing-kit:esnext": "./build/esnext/react/jsx-runtime.esnext",
      "import": "./build/esm/react/jsx-runtime.mjs",
      "require": "./build/cjs/react/jsx-runtime.cjs"
    },
    "./react/server": {
      "quilt:internal": "./src/react/server.ts",
      "sewing-kit:esnext": "./build/esnext/react/server.esnext",
      "import": "./build/esm/react/server.mjs",
      "require": "./build/cjs/react/server.cjs"
    },
    "./react": {
      "quilt:internal": "./src/react/index.ts",
      "sewing-kit:esnext": "./build/esnext/react/index.esnext",
      "import": "./build/esm/react/index.mjs",
      "require": "./build/cjs/react/index.cjs"
    },
    "./http-handlers/node": {
      "quilt:internal": "./src/http-handlers/node.ts",
      "sewing-kit:esnext": "./build/esnext/http-handlers/node.esnext",
      "import": "./build/esm/http-handlers/node.mjs",
      "require": "./build/cjs/http-handlers/node.cjs"
    },
    "./http-handlers": {
      "quilt:internal": "./src/http-handlers/index.ts",
      "sewing-kit:esnext": "./build/esnext/http-handlers/index.esnext",
      "import": "./build/esm/http-handlers/index.mjs",
      "require": "./build/cjs/http-handlers/index.cjs"
    },
    "./polyfills/base": {
      "quilt:internal": "./src/polyfills/base.ts",
      "sewing-kit:esnext": "./build/esnext/polyfills/base.esnext",
      "import": "./build/esm/polyfills/base.mjs",
      "require": "./build/cjs/polyfills/base.cjs"
    },
    "./polyfills/fetch": {
      "quilt:internal": "./src/polyfills/fetch.ts",
      "sewing-kit:esnext": "./build/esnext/polyfills/fetch.esnext",
      "import": "./build/esm/polyfills/fetch.mjs",
      "require": "./build/cjs/polyfills/fetch.cjs"
    },
    "./polyfills/noop": {
      "quilt:internal": "./src/polyfills/noop.ts",
      "sewing-kit:esnext": "./build/esnext/polyfills/noop.esnext",
      "import": "./build/esm/polyfills/noop.mjs",
      "require": "./build/cjs/polyfills/noop.cjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "server": [
        "./build/typescript/server/index.d.ts"
      ],
      "static": [
        "./build/typescript/static/index.d.ts"
      ],
      "html": [
        "./build/typescript/html.d.ts"
      ],
      "email": [
        "./build/typescript/email.d.ts"
      ],
      "global": [
        "./build/typescript/global.d.ts"
      ],
      "http": [
        "./build/typescript/http.d.ts"
      ],
      "testing": [
        "./build/typescript/testing.d.ts"
      ],
      "matchers": [
        "./build/typescript/matchers.d.ts"
      ],
      "react/jsx-runtime": [
        "./build/typescript/react/jsx-runtime.d.ts"
      ],
      "react/server": [
        "./build/typescript/react/server.d.ts"
      ],
      "react": [
        "./build/typescript/react/index.d.ts"
      ],
      "http-handlers/node": [
        "./build/typescript/http-handlers/node.d.ts"
      ],
      "http-handlers": [
        "./build/typescript/http-handlers/index.d.ts"
      ],
      "polyfills/base": [
        "./build/typescript/polyfills/base.d.ts"
      ],
      "polyfills/fetch": [
        "./build/typescript/polyfills/fetch.d.ts"
      ],
      "polyfills/noop": [
        "./build/typescript/polyfills/noop.d.ts"
      ],
      "magic/app": [
        "./build/typescript/magic/app.d.ts"
      ],
      "magic/asset-loader": [
        "./build/typescript/magic/asset-loader.d.ts"
      ],
      "magic/http-handler": [
        "./build/typescript/magic/http-handler.d.ts"
      ],
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": [
    "./build/esnext/global.esnext",
    "./build/esm/global.mjs"
  ],
  "dependencies": {
    "@quilted/async": "^0.3.12",
    "@quilted/graphql": "^0.4.14",
    "@quilted/http-handlers": "^0.2.20",
    "@quilted/polyfills": "^0.2.12",
    "@quilted/preact-mini-compat": "^0.2.7",
    "@quilted/react-async": "^0.3.22",
    "@quilted/react-email": "^0.2.18",
    "@quilted/react-graphql": "^0.4.30",
    "@quilted/react-html": "^0.3.18",
    "@quilted/react-http": "^0.3.8",
    "@quilted/react-idle": "^0.3.6",
    "@quilted/react-performance": "^0.1.0",
    "@quilted/react-router": "^0.3.28",
    "@quilted/react-server-render": "^0.3.7",
    "@quilted/react-testing": "^0.5.1",
    "@quilted/react-workers": "^0.2.12",
    "@quilted/useful-react-types": "^0.1.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "preact": "^10.5.0",
    "preact-render-to-string": "^5.0.0"
  },
  "devDependencies": {
    "@types/prettier": "^2.0.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "prettier": "^2.0.0",
    "react": ">=17.0.0 <18.0.0",
    "react-dom": ">=17.0.0 <18.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/react",
      "@quilted/eslint-config/package"
    ]
  }
}
