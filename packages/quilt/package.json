{
  "name": "@quilted/quilt",
  "type": "module",
  "version": "0.5.114",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/quilt"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
