    "./test-utils": {
      "quilt:source": "./source/test-utils.ts",
      "quilt:esnext": "./build/esnext/test-utils.esnext",
      "import": "./build/esm/test-utils.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config configuration/rollup.config.js"
