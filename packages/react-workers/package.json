{
  "name": "@quilted/react-workers",
  "type": "module",
  "version": "0.2.21",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/react-workers"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "quilt:from-source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./worker": {
      "quilt:from-source": "./source/worker.ts",
      "quilt:esnext": "./build/esnext/worker.esnext",
      "import": "./build/esm/worker.mjs",
      "require": "./build/cjs/worker.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "worker": [
        "./build/typescript/worker.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@quilted/workers": "^0.2.12"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "react": ">=17.0.0 <18.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/react",
      "@quilted/eslint-config/package"
    ]
  }
}
