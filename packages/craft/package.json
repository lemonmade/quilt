{
  "name": "@quilted/craft",
  "type": "module",
  "version": "0.1.70",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/craft"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "quilt:from-source": "./src/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./graphql": {
      "quilt:from-source": "./src/graphql.ts",
      "quilt:esnext": "./build/esnext/graphql.esnext",
      "import": "./build/esm/graphql.mjs",
      "require": "./build/cjs/graphql.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "graphql": [
        "./build/typescript/graphql.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/core": ">=7.0.0 <8.0.0",
    "@prefresh/vite": "^2.2.0",
    "@quilted/browserslist-config": "0.1.2",
    "@quilted/eslint-config": "0.1.5",
    "@quilted/polyfills": "0.2.16",
    "@quilted/prettier": "0.2.4",
    "@quilted/sewing-kit": "0.2.18",
    "@quilted/sewing-kit-babel": "0.1.9",
    "@quilted/sewing-kit-eslint": "0.1.6",
    "@quilted/sewing-kit-esnext": "0.1.20",
    "@quilted/sewing-kit-jest": "0.1.11",
    "@quilted/sewing-kit-package": "0.1.13",
    "@quilted/sewing-kit-prettier": "0.1.6",
    "@quilted/sewing-kit-react": "0.1.6",
    "@quilted/sewing-kit-rollup": "0.1.10",
    "@quilted/sewing-kit-targets": "0.1.10",
    "@quilted/sewing-kit-typescript": "0.1.7",
    "@quilted/sewing-kit-vite": "0.1.7",
    "@quilted/typescript": "0.2.5",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-replace": "^2.4.0",
    "browserslist": "^4.0.0",
    "browserslist-useragent-regexp": "^3.0.0",
    "clean-css": "^5.0.0",
    "common-tags": "^1.8.0",
    "connect": "^3.7.0",
    "core-js": "^3.0.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.12.0",
    "graphql-config": "^4.1.0",
    "pkg-dir": "^6.0.0",
    "postcss": "^8.0.0",
    "postcss-modules": "^4.0.0",
    "prettier": "^2.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-esbuild": "^4.8.0",
    "rollup-plugin-visualizer": "^5.5.0",
    "systemjs": "^6.10.2",
    "vite": "^2.0.0"
  },
  "devDependencies": {
    "@quilted/async": "^0.3.0",
    "@quilted/graphql": "^0.4.0",
    "@quilted/quilt": "^0.5.0",
    "@quilted/workers": "^0.2.0",
    "@types/clean-css": "^4.0.0",
    "@types/connect": "^3.4.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "@quilted/async": "^0.3.15",
    "@quilted/graphql": "^0.4.19",
    "@quilted/quilt": "^0.5.66",
    "@quilted/workers": "^0.2.14",
    "preact": "^10.5.0",
    "typescript": ">=4.0.0 <5.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "@quilted/async": {
      "optional": true
    },
    "@quilted/graphql": {
      "optional": true
    },
    "@quilted/polyfills": {
      "optional": true
    },
    "@quilted/workers": {
      "optional": true
    },
    "preact": {
      "optional": true
    },
    "typescript": {
      "optional": false
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/node",
      "@quilted/eslint-config/package"
    ]
  }
}
