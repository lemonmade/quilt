{
  "name": "@quilted/craft",
  "type": "module",
  "version": "0.1.120",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/craft"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org",
    "bin": {
      "quilt": "./bin/quilt.mjs"
    }
  },
  "exports": {
    ".": {
      "quilt:from-source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./babel": {
      "quilt:from-source": "./source/tools/babel.ts",
      "quilt:esnext": "./build/esnext/tools/babel.esnext",
      "import": "./build/esm/tools/babel.mjs",
      "require": "./build/cjs/tools/babel.cjs"
    },
    "./eslint": {
      "quilt:from-source": "./source/tools/eslint.ts",
      "quilt:esnext": "./build/esnext/tools/eslint.esnext",
      "import": "./build/esm/tools/eslint.mjs",
      "require": "./build/cjs/tools/eslint.cjs"
    },
    "./esnext": {
      "quilt:from-source": "./source/features/esnext.ts",
      "quilt:esnext": "./build/esnext/features/esnext.esnext",
      "import": "./build/esm/features/esnext.mjs",
      "require": "./build/cjs/features/esnext.cjs"
    },
    "./graphql": {
      "quilt:from-source": "./source/features/graphql.ts",
      "quilt:esnext": "./build/esnext/features/graphql.esnext",
      "import": "./build/esm/features/graphql.mjs",
      "require": "./build/cjs/features/graphql.cjs"
    },
    "./jest/resolver.cjs": {
      "require": "./jest/resolver.cjs"
    },
    "./jest/assets.cjs": {
      "require": "./jest/assets.cjs"
    },
    "./jest/styles.cjs": {
      "require": "./jest/styles.cjs"
    },
    "./jest/css-modules.cjs": {
      "require": "./jest/css-modules.cjs"
    },
    "./jest": {
      "quilt:from-source": "./source/tools/jest.ts",
      "quilt:esnext": "./build/esnext/tools/jest.esnext",
      "import": "./build/esm/tools/jest.mjs",
      "require": "./build/cjs/tools/jest.cjs"
