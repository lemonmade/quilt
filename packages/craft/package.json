    "eslint": "^7.27.0",
    "express": "^4.17.0",
    "globby": "^13.0.0",
    "jest": "^27.5.0",
    "jest-config": "^27.5.0",
    "jest-watch-typeahead": "^1.0.0",
    "mrmime": "^1.0.0",
    "pkg-dir": "^6.0.0",
    "postcss": "^8.0.0",
    "postcss-load-config": "^3.1.0",
    "postcss-modules": "^4.0.0",
    "postcss-preset-env": "^7.4.0",
    "preact": "^10.7.0",
    "prettier": "^2.6.0",
    "regenerator-runtime": "^0.13.0",
    "rollup": "^2.70.0",
    "rollup-plugin-esbuild": "^4.8.0",
    "rollup-plugin-node-externals": "^4.0.0",
