  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "kit": [
        "./build/typescript/kit.d.ts"
      ],
      "esnext": [
        "./build/typescript/features/esnext.d.ts"
      ],
      "graphql": [
        "./build/typescript/features/graphql.d.ts"
      ],
      "packages": [
        "./build/typescript/features/packages.d.ts"
      ],
      "polyfills": [
        "./build/typescript/features/polyfills.d.ts"
      ],
      "react": [
        "./build/typescript/features/react.d.ts"
      ],
      "targets": [
        "./build/typescript/features/targets.d.ts"
      ],
      "babel": [
        "./build/typescript/tools/babel.d.ts"
      ],
      "eslint": [
        "./build/typescript/tools/eslint.d.ts"
      ],
      "jest": [
        "./build/typescript/tools/jest.d.ts"
      ],
      "postcss": [
        "./build/typescript/tools/postcss.d.ts"
      ],
      "prettier": [
        "./build/typescript/tools/prettier.d.ts"
      ],
      "rollup": [
        "./build/typescript/tools/rollup.d.ts"
      ],
      "typescript": [
        "./build/typescript/tools/typescript.d.ts"
      ],
      "vite": [
        "./build/typescript/tools/vite.d.ts"
      ]
    }
  },
  "sideEffects": false,
