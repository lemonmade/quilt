{
  "name": "@quilted/async",
  "version": "0.2.0",
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "esnext": "index.esnext",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "esnext": "./index.esnext"
    },
    "./": "./",
    "./assets": {
      "import": "./assets.mjs",
      "require": "./assets.js",
      "esnext": "./assets.esnext"
    }
  },
  "sideEffects": false,
  "optionalDependencies": {
    "@babel/core": ">=7.0.0",
    "@babel/plugin-syntax-dynamic-import": ">=7.0.0",
    "@babel/traverse": ">=7.0.0",
    "@babel/types": ">=7.0.0",
    "@sewing-kit/plugin-javascript": "^0.3.4",
    "@sewing-kit/plugins": "^0.1.6"
  },
  "devDependencies": {
    "@types/babel__core": ">=7.0.0",
    "@types/babel__traverse": ">=7.0.0"
  }
}
