{
  "name": "@quilted/cloudflare",
  "type": "module",
  "version": "0.1.26",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/cloudflare"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "quilt:from-source": "./src/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./http-handlers": {
      "quilt:from-source": "./src/http-handlers.tsx",
      "quilt:esnext": "./build/esnext/http-handlers.esnext",
      "import": "./build/esm/http-handlers.mjs",
      "require": "./build/cjs/http-handlers.cjs"
    },
    "./sewing-kit": {
      "quilt:from-source": "./src/sewing-kit.ts",
      "quilt:esnext": "./build/esnext/sewing-kit.esnext",
      "import": "./build/esm/sewing-kit.mjs",
      "require": "./build/cjs/sewing-kit.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "http-handlers": [
        "./build/typescript/http-handlers.d.ts"
      ],
      "sewing-kit": [
        "./build/typescript/sewing-kit.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@quilted/http-handlers": "^0.2.20",
    "common-tags": "^1.8.0",
    "mime": "^2.5.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "@quilted/craft": "^0.1.75",
    "@quilted/quilt": "^0.5.72",
    "@quilted/sewing-kit": "^0.2.22",
    "react": ">=17.0.0 <18.0.0"
  },
  "devDependencies": {
    "@types/mime": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "@quilted/craft": {
      "optional": true
    },
    "@quilted/quilt": {
      "optional": true
    },
    "@quilted/sewing-kit": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/node",
      "@quilted/eslint-config/package"
    ]
  }
}
