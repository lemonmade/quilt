{
  "name": "@quilted/react-async",
  "type": "module",
  "version": "0.2.15",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/react-async"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "sewing-kit:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs"
    },
    "./server": {
      "sewing-kit:esnext": "./build/esnext/server.esnext",
      "import": "./build/esm/server.mjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "server": [
        "./build/typescript/server.d.ts"
      ],
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=17.0.0 <18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@quilted/async": "^0.2.7",
    "@quilted/react-html": "^0.2.11",
    "@quilted/react-server-render": "^0.2.6",
    "@quilted/use-subscription": "^0.1.7"
  },
  "eslintConfig": {
    "extends": [
      "../eslint/react",
      "../eslint/package"
    ]
  }
}
