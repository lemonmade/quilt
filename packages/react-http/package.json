{
  "name": "@quilted/react-http",
  "description": "Provides components and hooks for interacting with HTTP primitives.",
  "type": "module",
  "version": "0.3.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/react-http"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "quilt:internal": "./src/index.ts",
      "sewing-kit:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./server": {
      "quilt:internal": "./src/server.ts",
      "sewing-kit:esnext": "./build/esnext/server.esnext",
      "import": "./build/esm/server.mjs",
      "require": "./build/cjs/server.cjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "server": [
        "./build/typescript/server.d.ts"
      ],
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@quilted/http": "^0.2.8",
    "@quilted/react-html": "^0.3.17",
    "@quilted/react-server-render": "^0.3.6",
    "js-cookie": "^2.2.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "react": ">=17.0.0 <18.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.0"
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/react",
      "@quilted/eslint-config/package"
    ]
  }
}
