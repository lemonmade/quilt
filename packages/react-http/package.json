{
  "name": "@quilted/react-http",
  "description": "Provides components and hooks for interacting with HTTP primitives.",
  "type": "module",
  "version": "0.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/react-http"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./build/typescript/index.d.ts",
      "quilt:source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs"
    },
    "./server": {
      "types": "./build/typescript/server.d.ts",
      "quilt:source": "./source/server.ts",
      "quilt:esnext": "./build/esnext/server.esnext",
      "import": "./build/esm/server.mjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "server": [
        "./build/typescript/server.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config configuration/rollup.config.js"
  },
  "dependencies": {
    "@quilted/http": "^0.3.0",
    "@quilted/react-html": "^0.4.0",
    "@quilted/react-server-render": "^0.4.0",
    "@quilted/react-utilities": "^0.2.0",
    "js-cookie": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/js-cookie": "^2.2.0",
    "react": "workspace:@quilted/react@^18.2.0"
  }
}
