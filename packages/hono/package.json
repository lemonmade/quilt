{
  "name": "@quilted/hono",
  "type": "module",
  "version": "0.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/hono"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./build/typescript/index.d.ts",
      "quilt:source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs"
    },
    "./node": {
      "types": "./build/typescript/node.d.ts",
      "quilt:source": "./source/node.ts",
      "quilt:esnext": "./build/esnext/node.esnext",
      "import": "./build/esm/node.mjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "node": [
        "./build/typescript/node.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config configuration/rollup.config.js"
  },
  "dependencies": {
    "@quilted/http": "workspace:^0.3.0",
    "@quilted/routing": "workspace:^0.4.0",
    "@hono/node-server": "^1.17.1",
    "hono": "^4.0.0",
    "send": "^0.17.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/send": "^0.17.4",
    "hono": "^4.8.9"
  }
}
