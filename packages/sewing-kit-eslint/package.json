{
  "name": "@quilted/sewing-kit-eslint",
  "type": "module",
  "version": "0.1.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/sewing-kit-eslint"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "quilt:internal": "./src/index.ts",
      "sewing-kit:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "eslint": "^7.27.0"
  },
  "devDependencies": {
    "@quilted/sewing-kit": "^0.2.0",
    "@quilted/sewing-kit-prettier": "^0.1.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0 <8.0.0",
    "@quilted/sewing-kit": "^0.2.0",
    "@quilted/sewing-kit-prettier": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    },
    "@quilted/sewing-kit": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/node",
      "@quilted/eslint-config/package"
    ]
  }
}
