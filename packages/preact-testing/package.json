{
  "name": "@quilted/preact-testing",
  "type": "module",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/preact-testing"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./build/typescript/index.d.ts",
      "quilt:source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs"
    },
    "./matchers": {
      "types": "./build/typescript/matchers.d.ts",
      "quilt:source": "./source/matchers.ts",
      "quilt:esnext": "./build/esnext/matchers.esnext",
      "import": "./build/esm/matchers.mjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "matchers": [
        "./build/typescript/matchers/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config configuration/rollup.config.js"
  },
  "dependencies": {
    "jest-matcher-utils": "^27.0.0"
  },
  "devDependencies": {
    "@types/react-test-renderer": "^18.0.0",
    "expect": "^29.0.0",
    "preact": "^10.17.0"
  },
  "peerDependencies": {
    "preact": "^10.17.0"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/project",
      "@quilted/eslint-config/react"
    ]
  }
}
