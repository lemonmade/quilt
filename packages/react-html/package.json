{
  "name": "@quilted/react-html",
  "type": "module",
  "version": "0.2.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/react-html"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "sewing-kit:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs"
    },
    "./testing": {
      "sewing-kit:esnext": "./build/esnext/testing/index.esnext",
      "import": "./build/esm/testing/index.mjs"
    },
    "./server": {
      "sewing-kit:esnext": "./build/esnext/server/index.esnext",
      "import": "./build/esm/server/index.mjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "server": [
        "./build/typescript/server/index.d.ts"
      ],
      "testing": [
        "./build/typescript/testing/index.d.ts"
      ],
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=17.0.0 <18.0.0",
    "react-dom": ">=17.0.0 <18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@quilted/react-server-render": "^0.2.6",
    "@types/multistream": "^2.1.1",
    "@types/react": "^17.0.0",
    "multistream": "^4.0.1",
    "serialize-javascript": "^5.0.1"
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.3",
    "@types/serialize-javascript": "^5.0.0"
  },
  "eslintConfig": {
    "extends": [
      "../eslint/react",
      "../eslint/package"
    ]
  }
}
