{
  "name": "@quilted/trpc",
  "description": "Helpers for using tRPC together with Quilt",
  "type": "module",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "@quilted/registry": "https://registry.npmjs.org"
  },
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/trpc"
  },
  "exports": {
    ".": {
      "types": "./build/typescript/index.d.ts",
      "quilt:source": "./source/index.ts",
      "quilt:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./server": {
      "types": "./build/typescript/server.d.ts",
      "quilt:source": "./source/server.ts",
      "quilt:esnext": "./build/esnext/server.esnext",
      "import": "./build/esm/server.mjs",
      "require": "./build/cjs/server.cjs"
    }
  },
  "types": "./build/typescript/index.d.ts",
  "sideEffects": false,
  "dependencies": {},
  "peerDependencies": {
    "@trpc/client": "^10.0.0",
    "@trpc/server": "^10.0.0"
  },
  "peerDependenciesMeta": {
    "@trpc/client": {
      "optional": true
    },
    "@trpc/server": {
      "optional": true
    }
  },
  "devDependencies": {
    "@quilted/testing": "^0.1.0",
    "@trpc/client": "^10.31.0",
    "@trpc/server": "^10.31.0"
  },
  "eslintConfig": {
    "extends": [
      "@quilted/eslint-config/project"
    ]
  },
  "browserslist": [
    "extends @quilted/browserslist-config/defaults"
  ]
}
