{
  "name": "@quilted/polyfills",
  "version": "0.0.7",
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "main": "index.js",
  "module": "index.mjs",
  "esnext": "index.esnext",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./": "./",
    "./base": {
      "esnext": "./base.esnext",
      "import": "./base.mjs",
      "require": "./base.js"
    },
    "./fetch": {
      "esnext": "./fetch.esnext",
      "import": "./fetch.mjs",
      "require": "./fetch.js"
    },
    "./fetch.browser": {
      "esnext": "./fetch.browser.esnext",
      "import": "./fetch.browser.mjs",
      "require": "./fetch.browser.js"
    },
    "./fetch.node": {
      "esnext": "./fetch.node.esnext",
      "import": "./fetch.node.mjs",
      "require": "./fetch.node.js"
    },
    "./noop": {
      "esnext": "./noop.esnext",
      "import": "./noop.mjs",
      "require": "./noop.js"
    }
  },
  "license": "MIT",
  "sideEffects": true,
  "dependencies": {
    "@types/caniuse-api": "^3.0.0",
    "@types/node-fetch": "^2.0.0",
    "caniuse-api": "^3.0.0",
    "core-js": "^3.0.0",
    "magic-string": "^0.25.0",
    "node-fetch": "^2.0.0",
    "regenerator-runtime": "^0.13.0",
    "whatwg-fetch": "^3.5.0"
  },
  "peerDependencies": {
    "rollup": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  }
}
