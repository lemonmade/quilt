{
  "name": "@quilted/react-graphql",
  "type": "module",
  "version": "0.3.15",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonmade/quilt.git",
    "directory": "packages/react-graphql"
  },
  "publishConfig": {
    "access": "public",
    "@quilted:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "sewing-kit:esnext": "./build/esnext/index.esnext",
      "import": "./build/esm/index.mjs"
    },
    "./testing": {
      "sewing-kit:esnext": "./build/esnext/testing.esnext",
      "import": "./build/esm/testing.mjs"
    }
  },
  "typesVersions": {
    ">=1": {
      "testing": [
        "./build/typescript/testing.d.ts"
      ],
      "*": [
        "./build/typescript/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@quilted/graphql": "^0.3.10",
    "@quilted/react-async": "^0.2.15",
    "@quilted/useful-types": "^0.1.6"
  },
  "peerDependencies": {
    "react": ">=17.0.0 <18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "../eslint/react",
      "../eslint/package"
    ]
  }
}
